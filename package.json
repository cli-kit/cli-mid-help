{
  "name": "cli-mid-help",
  "version": "1.0.17",
  "description": "Help option middleware",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/cli-mid-help"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/cli-mid-help/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/freeformsystems/cli-mid-help/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "cli",
    "middleware",
    "help"
  ],
  "dependencies": {
    "cli-env": "~1.0.10",
    "cli-help": "~0.1.1"
  },
  "devDependencies": {
    "chai": "~3.2.0",
    "istanbul": "~0.3.17",
    "mocha": "~2.2.5"
  },
  "engine": [
    "node >= 0.10.21"
  ],
  "scripts": {
    "readme": "mdp --force -v",
    "test": "istanbul cover _mocha -- -u bdd --recursive --bail --reporter list -A test/spec"
  },
  "mdp": {
    "title": "Help Middleware",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/freeformsystems/cli-mid-help",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "test.md"
        ]
      },
      {
        "inc": [
          "license.md"
        ]
      }
    ]
  }
}
